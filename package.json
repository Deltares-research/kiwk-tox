{
  "name": "kiwk-tox",
  "version": "1.0.0",
  "description": "KIWK TOX",
  "browserslist": [
    "defaults"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "revision": "node config/build-scripts/revision.js",
    "eleventy:dev": "cross-env ELEVENTY_ENV=development eleventy --serve",
    "eleventy:prod": "cross-env ELEVENTY_ENV=production eleventy",
    "dev": "run-s clean eleventy:dev --print-label",
    "start": "npm run dev",
    "build": "run-s clean eleventy:prod --print-label",
    "postbuild": "npm run revision"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/voorhoede/KIWK-TOX"
  },
  "author": "De Voorhoede",
  "license": "GNU",
  "dependencies": {
    "@wildpow/datocms-search": "0.0.5",
    "focus-trap": "^5.1.0",
    "focus-visible": "^5.2.0",
    "list.js": "^2.3.1",
    "lodash.debounce": "^4.0.8",
    "query-string": "^6.13.7",
    "ramda": "^0.27.1"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@babel/core": "^7.11.4",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "babel-loader": "^8.2.2",
    "clean-css": "^4.2.3",
    "critical": "^2.0.3",
    "cross-env": "^7.0.2",
    "cssesc": "^3.0.0",
    "dotenv-safe": "^8.2.0",
    "gulp": "^4.0.2",
    "gulp-filter": "^6.0.0",
    "gulp-rev": "^9.0.0",
    "gulp-rev-css-url": "^0.1.0",
    "gulp-rev-delete-original": "^0.2.3",
    "gulp-rev-replace": "^0.4.4",
    "html-minifier": "^4.0.0",
    "luxon": "^1.25.0",
    "memfs": "^3.2.0",
    "node-fetch": "^2.6.1",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "svg-sprite": "^1.5.0",
    "webpack": "^4.44.1"
  }
